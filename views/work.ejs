<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>MM Studio</title>
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,100..900;1,100..900&family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">
  <link rel="shortcut icon" href="/pic and vid/Musik.png" type="image/x-icon" />
  <link rel="stylesheet" href="https://unpkg.com/swiper/swiper-bundle.min.css" />
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/locomotive-scroll@3.5.4/dist/locomotive-scroll.css" />
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.css" />
  <link rel="stylesheet" href="./css/work.css" />
  <link rel="stylesheet" href="./css/home.css" />
</head>

<body class="body">
  <div class="main">
    <div class="page-1">
      <div class="background">
        <img src="/pic and vid/DSC00063.JPG" alt="" sizes="(max-width: 600px) 400px, 
       (max-width: 1200px) 800px, 
       1200px" alt="Optimized image" />
      </div>
       
      <div class="navbar">
        <div class="l-side">
          <img src="/pic and vid/MM Logo12.png" alt="Logo" sizes="(max-width: 600px) 400px, (max-width: 1200px) 800px, 1200px" />
        </div>
        <div class="r-side">
          <a href="/home"><button>Home</button></a>
          <a href="/work"><button>Work</button></a>
          <!-- <a href="/about"><button>About Us</button></a> -->
          <a href="/contact"><button>Contact Us</button></a>
        </div>
        <div class="hamburger" id="hamburger">
          <div></div>
          <div></div>
          <div></div>
        </div>
      </div>
    
      <div class="popup-overlay" id="popup-overlay"></div>
      <div class="popup" id="popup">
        <a href="/home">Home</a>
        <a href="/work">Work</a>
        <!-- <a href="/about">About Us</a> -->
        <a href="/contact">Contact Us</a>
      </div>
      <div class="txt">
        <p>Welcome To Our Project Page</p>
      </div>
    </div>
    <!-- <button id="get-started">Get Started</button> -->
    <div class="our">
      <div class="latest">
        <h1>Our latest Projects</h1>
      </div>
      <!-- Swiper -->
      <div class="swiper mySwiper">
        <div class="swiper-wrapper">
          <!-- Loop through the swiper slides and wrap each image in an anchor tag -->
          <% if (swiperSlides && swiperSlides.length > 0) { %>
            <% swiperSlides.forEach(slide => { %>
              <div class="swiper-slide">
                <a href="<%= slide.anchorUrl %>" target="_blank">
                  <img src="<%= slide.imageUrl %>" alt="Swiper Slide Image" class="simg">
                </a>
              </div>
            <% }); %>
          <% } else { %>
            <p>No swiper slides available.</p>
          <% } %>
        </div>
        <!-- Add navigation and pagination if necessary -->
      </div>
      
    <!-- new card testing -->


    <div class="card-section1">
      <% containers.forEach(container=> { %>
        <div class="card-container1">
          <div class="cc-h1">
            <h1>
              <%= container.title %>
            </h1>
          </div>
          <div class="cards1 ">
            <% container.cards.forEach(card=> { %>
              <div class="card1">
                <img src="<%= card.imageUrl %>" alt="<%= card.title %>">
                <div class="txt567">
                  <div class="txt5">
                    <p>
                      <%= card.title %>
                    </h3>
                  </div>
                  <div class="txt6">
                    <p>
                      <%= card.paragraph %>
                    </p>
                  </div>
                  <div class="txt7">
                    <a href="<%= card.linkUrl %>" target="_blank">Watch Video</a>
                  </div>
                </div>
              </div>
              <% }); %>
          </div>
        </div>
        <% }); %>
    </div>
  </div>
  <div class="page-6">
    <div class="p-left">
      <div class="re">
      <div class="reach">
        <h1>Reach Out Today</h1>
      </div>
      <div class="this-form">
        <p>
          Contact us through this form. We are help you with music needs and
          questions
        </p>
      </div>
      </div>
    </div>
    <div class="p-right">
      <div class="form-container">
        <h1>Contact Us</h1>
        <form action="/send-mail" method="POST">
            <label for="name">Name</label>
            <input type="text" id="name" name="name" required>

            <label for="email">Email</label>
            <input type="email" id="email" name="email" required>

            <label for="mobile">Mobile Number</label>
            <input type="text" id="mobile" name="mobile" required>

            <label for="message">Message</label>
            <textarea id="message" name="message" rows="5" required></textarea>

            <button type="submit">Send Message</button>
        </form>
    </div>

    </div>
  </div>
  <footer>
    <div class="blackbar"></div>
  </footer>
  </div>
  <script src="https://unpkg.com/swiper/swiper-bundle.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/locomotive-scroll@3.5.4/dist/locomotive-scroll.js"></script>
  <script src="/js/work.js"></script>
</body>

</html>